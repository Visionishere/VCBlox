{
  "manifest_version": 2,
  "name": "VCBlox",
  "version": "1.0",
  "permissions": [
    "https://voice.roblox.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://voice.roblox.com; object-src 'self'",
  "web_accessible_resources": ["icon.png", "banned_icon.png"]
}
